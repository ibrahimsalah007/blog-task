{
  "name": "todo-task",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && npx typeorm -d dist/core/database/typeorm-migration-datasource.js",
    "typeorm:seeder": "npm run build && npx typeorm -d dist/core/database/typeorm-seeder-datasource.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:create": "npx typeorm migration:create",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "seeder:create": "npx typeorm migration:create",
    "seeder:run": "npm run typeorm:seeder -- migration:run",
    "seeder:revert": "npm run typeorm:seeder -- migration:revert",
    "db:migrate": "npm run build && npx typeorm -d dist/core/database/typeorm-migration-datasource.js migration:run && npx typeorm -d dist/core/database/typeorm-seeder-datasource.js migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.387.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/geojson": "^7946.0.10",
    "@types/otp-generator": "^4.0.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "api": "^6.1.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "generate-password": "^1.7.1",
    "joi": "^17.9.2",
    "libphonenumber-js": "^1.10.50",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mongoose": "^8.0.3",
    "nestjs-cls": "^3.5.1",
    "nestjs-i18n": "^10.2.6",
    "nestjs-pino": "^3.2.0",
    "nodemailer": "^6.9.5",
    "otp-generator": "^4.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^8.3.3",
